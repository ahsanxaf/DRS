@model IEnumerable<DRS.Models.LogViewModel>

@{
    ViewData["Title"] = "Logs";
}

<h1>Logs</h1>

<table class="table">
    <thead>
        <tr>
            <th>Log ID</th>
            <th>User Name</th>
            <th>Action</th>
            <th>Document Title</th>
            <th>Timestamp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.LogId</td>
                <td>@log.UserName</td>
                <td>@log.Action</td>
                <td>@log.DocumentTitle</td>
                <td>@log.Timestamp</td>
            </tr>
        }
    </tbody>
</table>
